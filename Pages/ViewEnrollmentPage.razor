@page "/viewEnrollmentpage"

@if(ViewEnrollments == null){
    <div class="spinner"></div>
}
else
{


<div class="card-deck">

    @foreach (var emp in ViewEnrollments)
{
  <div class="card m-3" style="min-width: 18rem; max-width:30%">


<div class="card-header">
<h3>@emp.firstName @emp.lastName</h3>
</div>
<div class="card-footer text-center">
    <a href="@($"viewEnrollmentsdetail/{emp.studentID}")" class="btn btn-primary m-1">view</a>
 <a href="@($"editviewEnrollments/{emp.studentID}")" class="btn btn-primary m-1">edit</a>
  <a href="#" class="btn btn-primary m-1">delete</a>

</div>

  </div>


}
</div>

}